{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowS3Access",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:DeleteObject",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::scale-r-pms-storage-prod",
                "arn:aws:s3:::scale-r-pms-storage-prod/*"
            ]
        }
    ]
}