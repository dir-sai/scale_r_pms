{
  "expo": {
    "name": "scale-r-pms",
    "slug": "scale-r-pms",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "scale-r-pms",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.scale-r.pms",
      "config": {
        "googleSignIn": {
          "reservedClientId": "93305427002-uqgn235jofo5l2cdel7jgpn5180i3p5f.apps.googleusercontent.com"
        }
      }
    },
    "android": {
      "package": "com.scale_r.pms",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "config": {
        "googleSignIn": {
          "clientId": "93305427002-uqgn235jofo5l2cdel7jgpn5180i3p5f.apps.googleusercontent.com"
        }
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "@react-native-google-signin/google-signin",
      "expo-secure-store",
      "expo-auth-session",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7fe59c35-18a1-4151-a7eb-6254f3ff1f10"
      }
    },
    "owner": "dir-sai"
  }
}